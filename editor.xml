<application>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="SHOW_METHOD_SEPARATORS" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
    <pattern useCustomColors="true" pattern="\b(NOTE_BY_ZWC|fixme)\b.*">
      <option name="FOREGROUND" value="6ff1b" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
  </component>
</application>

